{
  "name": "reconnect/vault",
  "type": "project",
  "license": "MIT",
  "description": "A vault to store any document",
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.1",
    "ext-apcu": "*",
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-fileinfo": "*",
    "ext-iconv": "*",
    "ext-imagick": "*",
    "ext-openssl": "*",
    "ext-zip": "*",
    "api-platform/core": "3.1.12",
    "beberlei/doctrineextensions": "1.3.0",
    "doctrine/doctrine-bundle": "2.10.*",
    "doctrine/doctrine-migrations-bundle": "3.2.*",
    "friendsofsymfony/ckeditor-bundle": "2.4.*",
    "friendsofsymfony/jsrouting-bundle": "3.3.*",
    "friendsofsymfony/rest-bundle": "^3.5",
    "gedmo/doctrine-extensions": "3.13.*",
    "knplabs/knp-menu-bundle": "3.2.*",
    "knplabs/knp-paginator-bundle": "5.9.*",
    "league/csv": "9.11.*",
    "league/oauth2-server-bundle": "0.5.*",
    "lexik/jwt-authentication-bundle": "^2.16",
    "liip/imagine-bundle": "2.12.*",
    "maennchen/zipstream-php": "2.4.*",
    "mailjet/mailjet-apiv3-php": "1.6.*",
    "nelmio/cors-bundle": "2.3.*",
    "phpdocumentor/reflection-docblock": "5.3.*",
    "phpoffice/phpspreadsheet": "1.29.*",
    "ramsey/uuid": "4.7.*",
    "reconnect/s3bundle": "1.3.*",
    "sonata-project/admin-bundle": "4.27.*",
    "sonata-project/doctrine-orm-admin-bundle": "4.13.*",
    "sonata-project/intl-bundle": "3.1.*",
    "stof/doctrine-extensions-bundle": "1.8.*",
    "symfony/asset": "6.*",
    "symfony/brevo-mailer": "6.4.*",
    "symfony/console": "6.*",
    "symfony/crowdin-translation-provider": "6.*",
    "symfony/dotenv": "6.*",
    "symfony/expression-language": "6.*",
    "symfony/flex": "2.3.*",
    "symfony/form": "6.*",
    "symfony/framework-bundle": "6.*",
    "symfony/html-sanitizer": "6.*",
    "symfony/http-client": "6.*",
    "symfony/http-kernel": "6.*",
    "symfony/intl": "6.*",
    "symfony/mailer": "6.*",
    "symfony/mailjet-mailer": "6.*",
    "symfony/mailjet-notifier": "6.*",
    "symfony/monolog-bundle": "3.8.*",
    "symfony/notifier": "6.*",
    "symfony/polyfill-apcu": "1.28.*",
    "symfony/process": "6.*",
    "symfony/property-access": "6.*",
    "symfony/property-info": "6.*",
    "symfony/proxy-manager-bridge": "6.*",
    "symfony/psr-http-message-bridge": "2.3.*",
    "symfony/rate-limiter": "6.*",
    "symfony/runtime": "6.*",
    "symfony/security-bundle": "6.*",
    "symfony/serializer": "6.*",
    "symfony/translation": "6.*",
    "symfony/twig-bundle": "6.*",
    "symfony/ux-live-component": "2.12.*",
    "symfony/ux-turbo": "2.12.*",
    "symfony/validator": "6.*",
    "symfony/vonage-notifier": "6.*",
    "symfony/web-link": "6.*",
    "symfony/webpack-encore-bundle": "1.16.*",
    "symfony/yaml": "6.*",
    "symfonycasts/reset-password-bundle": "1.18.*",
    "twig/extra-bundle": "3.7.*",
    "twig/intl-extra": "3.7.*",
    "vich/uploader-bundle": "2.2.*"
  },
  "require-dev": {
    "dama/doctrine-test-bundle": "7.2.*",
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "easycorp/easy-deploy-bundle": "dev-master",
    "fakerphp/faker": "1.23.*",
    "friendsofphp/php-cs-fixer": "3.21.*",
    "phpstan/phpstan": "^1.3",
    "phpstan/phpstan-symfony": "^1.0",
    "rector/rector": "0.17.1",
    "symfony/browser-kit": "6.*",
    "symfony/css-selector": "6.*",
    "symfony/debug-bundle": "6.*",
    "symfony/maker-bundle": "v1.51.*",
    "symfony/phpunit-bridge": "^6.0",
    "symfony/stopwatch": "6.*",
    "symfony/web-profiler-bundle": "6.*",
    "zenstruck/foundry": "^1.22"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/flex": true,
      "symfony/runtime": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "6.4.*"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:re-connect/easy-deploy-bundle.git"
    }
  ]
}
