{% extends '@Twig/Exception/generic_page.html.twig' %}

{% block body %}
    <div class="container pt-5 justify-content-center">
        {% set code = 500 %}
        <h1>
            {{ 'maintenance'|trans }}
        </h1>
        <div class="img-container">
            <img src="{{ asset('img/error/'~code~'.png') }}" alt="{{ 'error'~code }}" class="img-fluid image mx-auto"/>
        </div>
    </div>
{% endblock %}
