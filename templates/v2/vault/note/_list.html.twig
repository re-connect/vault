<table class="table table-100 table-hover spaced-table mb-0">
    <thead>
    <tr class="text-primary text-center font-size-1-3">
        <th class="bg-grey borderless text-start ps-md-3">{{ 'title'|trans }}</th>
        {% if app.user.isBeneficiaire %}
            <th class="bg-grey borderless pe-3">{{ 'access'|trans }}</th>
        {% endif %}
        <th class="bg-grey borderless pe-3">{{ 'actions'|trans }}</th>
    </tr>
    </thead>
    <tbody>
    {% for note in notes %}
        {{ include('v2/vault/note/_card.html.twig', {note}) }}
    {% endfor %}
    </tbody>
</table>
<hr class="text-primary">
<div class="d-flex justify-content-end">
    {{ knp_pagination_render(notes) }}
</div>
